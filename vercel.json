  {
    "version": 2,
    "builds": [
      {
        "src": "/api/submit.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/submit.js",
        "dest": "/api/submit.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }